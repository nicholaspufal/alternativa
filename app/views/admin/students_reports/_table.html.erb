<% if group.exams.any? %>
	<table class="table table-bordered table-condensed table-striped reports_table">
		<tbody>
			<% group.exams.ordered.each do |exam| %>
			<tr>
				<td>
					<%= student_report_presenter.exam_title(exam) %>
				</td>
				<td>
					<%= student_report_presenter.show_result(exam) %>
				</td>
			</tr>
			<% end %>	
		</tbody>		
	</table>
<% else %>
	<p>Ainda não foram gerados relatórios para este grupo deste aluno.</p>
<% end %>